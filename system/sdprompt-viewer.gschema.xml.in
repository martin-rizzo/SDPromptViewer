<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema gettext-domain="@GETTEXT_PACKAGE@"
          id="org.gnome.eog.plugins.sdprompt-viewer"
          path="/org/gnome/eog/plugins/sdprompt-viewer/">
   
  <key name="show-unknown-params" type="b">
    <default>false</default>
    <summary>Show unknown generation parameters</summary>
    <description>Enable to show generation parameters that were not identified by the parser.</description>
  </key>

  <key name="force-minimum-width" type="b">
    <summary>Force minimum width for the sidebar</summary>
    <description>
      If set to true, the minimum width of the sidebar will be forced to the value specified in 'sidebar-minimum-width'.
    </description>
    <default>true</default>
  </key>

  <key name="minimum-width" type="d">
    <summary>Sidebar minimum width</summary>
    <description>
      The minimum width of the sidebar. This value is used only if 'force-minimum-width' is set to true.
    </description>
    <default>480</default>
    <range min="100" max="1000"/>
  </key>   

  <key name="force-visibility" type="b">
    <default>false</default>
    <summary>Force visibility of this plugin in the sidebar</summary>
    <description>When this option is enabled, this plugin will be shown in the sidebar when EOG is started.</description>
  </key>

    </schema>
</schemalist>
